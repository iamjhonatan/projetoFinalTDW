load();\r\n            }\r\n        })\r\n    }\r\n\r\n    createImageStyle(){\r\n\r\n    }\r\n\r\n    render(){\r\n        const title = this.props.title;\r\n        const alt = this.props.alt || title;\r\n        return (\r\n          <div className=\"ImageLoader\">\r\n              <div className=\"control\">\r\n                  <input type=\"input\" className=\"txtUrl\" value={this.state.url} onChange={this.urlChange} placeholder=\"Enter the image URL here!\"/>\r\n                  <input type=\"button\" className=\"btnUrl\" value=\"Get\" onClick={this.replaceImage}/>\r\n              </div>\r\n              <img style={this.createImageStyle()} src={this.state.image} title={title} alt={alt}/>\r\n              {!this.state.downloading || <div className=\"barContainer\"><ProgressBar width=\"auto\" height=\"18px\" percent={this.state.loaded}/></div>}\r\n          </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default ImageLoader;\r\n",["107","108"],"C:\\Users\\jhona\\Documents\\PUC\\quartoPeriodo\\tdw\\projeto-final\\src\\page\\PostList\\index.js",[],["109","110"],"C:\\Users\\jhona\\Documents\\PUC\\quartoPeriodo\\tdw\\projeto-final\\src\\firebase.js",["111","112"],"C:\\Users\\jhona\\Documents\\PUC\\quartoPeriodo\\tdw\\projeto-final\\src\\serviceWorker.js",[],"C:\\Users\\jhona\\Documents\\PUC\\quartoPeriodo\\tdw\\projeto-final\\src\\page\\LoginForm\\index.js",["113","114","115","116","117","118"],"import React from 'react';\r\nimport './LoginForm.css';\r\nimport {auth} from '../../firebase';\r\nimport {HashRouter as Router, Switch, Route, Link, Redirect, withRouter} from \"react-router-dom\";\r\n\r\n\r\nclass LoginForm extends React.Component {\r\n\r\n    constructor(props) {\r\n        super(props);\r\n\r\n        this.state = {\r\n            user: null,\r\n            txtEmail: \"\",\r\n            txtPassword: \"\",\r\n            message: \".\"\r\n        }\r\n        this.onUpdate = this.onUpdate.bind(this);\r\n        this.onLogin = this.onLogin.bind(this)